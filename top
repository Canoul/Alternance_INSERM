[32m+[m
[32m+[m[32m```{r, echo = FALSE}[m
[32m+[m[32m# glm[m
[32m+[m
[32m+[m[32mglm_reg = function(y,x,data){[m
[32m+[m[32m  glm(formula=reformulate(termlabels = x, response= y), data=data, family = binomial(logit))[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mglm_m = function(y,x,data){[m
[32m+[m[32m  base = c("age","sexe","niveau_etude_reg","mmse","ch","dia","hta_med","imc_reg")[m
[32m+[m[32m  x = c(base,x)[m
[32m+[m[32m  n0 = length(base)[m
[32m+[m[32m  n1 = length(x)-n0[m
[32m+[m[32m  mod = glm_reg(y,x,data)[m
[32m+[m[32m  inf = length(mod$coefficients)-n1+1[m
[32m+[m[32m  sup = length(mod$coefficients)[m
[32m+[m[32m  coef(summary(mod))[inf:sup,][m
[32m+[m[32m  coef_table = round(coef(summary(mod))[inf:sup,c(1,4)],3)[m
[32m+[m[41m  [m
[32m+[m[32m  coef_table[1] <- round(exp(coef_table[1]),2)[m
[32m+[m
[32m+[m[41m  [m
[32m+[m[32m  if(n1 == 1){[m
[32m+[m[32m    names(coef_table) <- c("Odds Ratio", "p-value")[m
[32m+[m[32m  }else{[m
[32m+[m[32m    colnames(coef_table) <- c("Odd Ratio", "p-value")[m
[32m+[m[32m  }[m
[32m+[m[41m  [m
[32m+[m[32m  return(coef_table)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m```{r, echo = FALSE}[m
[32m+[m[32m# lm[m
[32m+[m
[32m+[m[32mlm_reg = function(y,x,data){[m
[32m+[m[32m  lm(formula=reformulate(termlabels = x, response= y), data=data)[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlm_m = function(y,x,data){[m
[32m+[m[32m  base = c("age","sexe","niveau_etude_reg","mmse","ch","dia","hta_med","imc_reg")[m
[32m+[m[32m  x = c(base,x)[m
[32m+[m[32m  n0 = length(base)[m
[32m+[m[32m  n1 = length(x)-n0[m
[32m+[m[32m  mod = lm_reg(y,x,data)[m
[32m+[m[32m  inf = length(mod$coefficients)-n1+1[m
[32m+[m[32m  sup = length(mod$coefficients)[m
[32m+[m[32m  coef(summary(mod))[inf:sup,][m
[32m+[m[32m  coef_table = round(coef(summary(mod))[inf:sup,c(1,4)],3)[m
[32m+[m[41m  [m
[32m+[m[32m  return(coef_table)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
[32m+[m
[32m+[m[32m```{r, echo = FALSE}[m
[32m+[m[32mdata$apoe.reg3[data$apoe.reg2 == 0] = 0[m
[32m+[m[32mdata$apoe.reg3[data$apoe.reg2 == 1] = 1[m
[32m+[m[32mdata$apoe.reg3[data$apoe.reg2 == 2] = 1[m
[32m+[m
[32m+[m[32mdata$ma2 <- FALSE[m
[32m+[m[32mdata$ma2[data$diagnostic_cli == 1] <- 1[m
[32m+[m[32mdata$ma2[data$diagnostic_cli == 2] <- 2[m
[32m+[m
[32m+[m
[32m+[m[32mdata$ma4[data$diagnostic_cli == 1] = 1[m
[32m+[m[32mdata$ma4[data$diagnostic_cli > 1] = 0[m
[32m+[m
[32m+[m[32mdata$ma = data$ma4[m
[32m+[m
[32m+[m
[32m+[m[32mdata$groupe[data$diagnostic_cli == 1] <- 1[m
[32m+[m[32mdata$groupe[data$diagnostic_cli == 2] <- 2[m
[32m+[m[32mdata$groupe[data$diagnostic_cli == 3] <- 3[m
[32m+[m[32mdata$groupe[data$diagnostic_cli > 3] <- 4[m
[32m+[m[32m```[m
[32m+[m
[32m+[m
[32m+[m[32m# Objectif[m
[32m+[m
[32m+[m[32mTrouver d'√©ventuelles associations en les marqueurs de la maladie d'Alzheimer et le Cholesterol (total, HDL et LDL)[m[41m  [m
[32m+[m[32m<br>[m
[32m+[m[32mLes valeurs du cholesterol sont standardis√©s[m[41m  [m
[32m+[m[32mLes valeurs de l'Abeta42 en continues sont standardis√©es sur leur logarithme et multipli√© par -1[m[41m  [m
[32m+[m[32mLes valeurs de ptau en continues sont standardis√©es sur leur logarithme et multipli√© par -1[m[41m  [m
[32m+[m
[32m+[m
[32m+[m[32m# Etude d'association[m
[32m+[m
[32m+[m[32m## Abeta42 en dichotomique {.tabset}[m
[32m+[m
[32m+[m[32m### Mod√®le de base[m
[32m+[m
[32m+[m[32mLe mod√®le de base comporte les variables suivantes :[m[41m  [m
[32m+[m[32m- Age[m[41m  [m
[32m+[m[32m- Sexe[m[41m  [m
[32m+[m[32m- Niveau d'√©tude[m[41m  [m
[32m+[m[32m- MMSE[m[41m  [m
[32m+[m[32m- Les traitements[m[41m  [m
[32m+[m[32m- IMC[m[41m  [m
[32m+[m[32m- APOE[m[41m  [m
[32m+[m
[32m+[m[32m```{r, echo = FALSE}[m
[32m+[m[32m#data_chol abeta42 dichotomique[m
[32m+[m
[32m+[m[32mdata_chol = filter(data, is.na(abeta42t2) == F,[m
[32m+[m[32m               is.na(age) == F,[m
[32m+[m[32m               is.na(sexe) == F,[m
[32m+[m[32m               is.na(apoe.reg2) == F,[m
[32m+[m[32m               is.na(mmse) == F,[m
[32m+[m[32m               is.na(ch) == F,[m
[32m+[m[32m               is.na(dia) == F,[m
[32m+[m[32m               is.na(hta_med) == F,[m
[32m+[m[32m               is.na(imc_reg) == F,[m
[32m+[m[32m               is.na(presence_biomarqueur) == F,[m
[32m+[m[32m               is.na(niveau_etude_reg) == F,[m
[32m+[m[32m               is.na(ma) == F[m
[32m+[m[32m               )[m
[32m+[m
[32m+[m[32mdata_chol$cholesterol_total = scale(data_chol$cholesterol_total)[m
[32m+[m[32mdata_chol$cholesterol_hdl = scale(data_chol$cholesterol_hdl)[m
[32m+[m[32mdata_chol$cholesterol_ldl = scale(data_chol$cholesterol_ldl)[m
[32m+[m
[32m+[m[32mdata_chol$sexe = as.factor(data_chol$sexe)[m
[32m+[m[32mdata_chol$ch = as.factor(data_chol$ch)[m
[32m+[m[32mdata_chol$dia = as.factor(data_chol$dia)[m
[32m+[m[32mdata_chol$hta_med = as.factor(data_chol$hta_med)[m
[32m+[m[32mdata_chol$apoe.reg2 = as.factor(data_chol$apoe.reg2)[m
[32m+[m[32mdata_chol$ma = as.factor(data_chol$ma)[m
[32m+[m[32mdata_chol$ma_mci = as.factor(data_chol$ma_mci)[m
[32m+[m
[32m+[m
[32m+[m[32mdata_ma_0 = data_chol %>% filter(ma == 0)[m
[32m+[m[32mdata_ma_1 = data_chol %>% filter(ma == 1)[m
[32m+[m
[32m+[m[32mdata_mamci_0 = data_chol %>% filter(ma_mci == 0)[m
[32m+[m[32mdata_mamci_1 = data_chol %>% filter(ma_mci == 1)[m
[32m+[m
[32m+[m[32mdata_apoe_0 = data_chol %>% filter(apoe.reg2 == 0)[m
[32m+[m[32mdata_apoe_1 = data_chol %>% filter(apoe.reg2 != 0)[m
[32m+[m
[32m+[m[32mdata_ma2_0 = data_chol %>% filter(ma2 == 0)[m
[32m+[m[32mdata_ma2_1 = data_chol %>% filter(ma2 == 1)[m
[32m+[m[32mdata_ma2_2 = data_chol %>% filter(ma2 == 2)[m
[32m+[m
[32m+[m
[32m+[m[32mdata_groupe_1 = data_chol %>% filter(groupe == 1)[m
[32m+[m[32mdata_groupe_2 = data_chol %>% filter(groupe == 2)[m
[32m+[m[32mdata_groupe_3 = data_chol %>% filter(groupe == 3)[m
[32m+[m[32mdata_groupe_4 = data_chol %>% filter(groupe == 4)[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
